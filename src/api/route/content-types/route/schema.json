      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "length": {
      "type": "decimal"
    },
    "stupanie": {
      "type": "integer"
    },
    "klesanie": {
      "type": "integer"
    },
    "map": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "subtitle": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "tourType": {
      "type": "enumeration",
      "enum": [
        "okruh",
        "tamSpat",
        "prechod"
      ]
    },
    "dayNum": {
      "type": "integer"
    },
    "level": {
      "type": "enumeration",
