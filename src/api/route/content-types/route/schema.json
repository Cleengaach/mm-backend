      "target": "api::mountain.mountain"
    },
    "region": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::region.region"
    },
    "Route_path": {
      "displayName": "route_path",
      "type": "component",
      "repeatable": true,
      "component": "location.route-path"
    },
    "Authors": {
      "displayName": "Authors",
      "type": "component",
      "repeatable": true,
      "component": "authors.authors"
    },
    "time": {
      "type": "string"
    }
