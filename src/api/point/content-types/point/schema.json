{
  "kind": "collectionType",
  "collectionName": "points",
  "info": {
    "singularName": "point",
    "pluralName": "points",
    "displayName": "Point"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "photogallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "altitude": {
      "type": "integer"
    },
    "description": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "north": {
      "type": "float"
    },
    "east": {
      "type": "float"
    },
    "mountain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mountain.mountain"
    }
  }
}
